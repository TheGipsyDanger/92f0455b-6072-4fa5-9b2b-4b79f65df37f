{
  "expo": {
    "name": "Pantore",
    "slug": "Pantore",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
    "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.thegipsydanger.pantore"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "android": {
      "package": "com.thegipsydanger.pantore"
    },
    "jsEngine": "hermes",
    "plugins": [
      [
        "expo-media-library",
        {
          "photosPermission": "O $(PRODUCT_NAME) precisa de acesso a suas fotos.",
          "savePhotosPermission": "O $(PRODUCT_NAME) precisa salvar arquivos.",
          "isAccessMediaLocationEnabled": true
        }
      ]
    ]
  }
}
